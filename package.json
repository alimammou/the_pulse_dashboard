{
    "name": "@xonboard/novatrack-iot-platform",
    "author": "XOnBoard",
    "private": true,
    "scripts": {
        "dev": "cross-var $npm_execpath run development",
        "development": "mix",
        "watch": "cross-var mix watch",
        "watch-poll": "cross-var mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "cross-var $npm_execpath run production",
        "production": "mix --production",
        "test": "cross-env NODE_ENV=test jest",
        "tdd": "cross-var $npm_execpath run test -- --watch --notify"
    },
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@coreui/coreui": "^2.1.12",
        "@fortawesome/fontawesome-free": "^5.9.0",
        "@tailwindcss/forms": "^0.3.1",
        "@tailwindcss/typography": "^0.4.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "alpinejs": "^3.0.6",
        "axios": "^0.19",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.6",
        "bootstrap": "^4.3.1",
        "cross-env": "^7.0",
        "cross-var": "^1.1.0",
        "cssnano": "^4.1.10",
        "eslint": "^6.7.2",
        "jest": "^24.8.0",
        "jquery": "^3.4.1",
        "laravel-mix": "^6.0.25",
        "lodash": "^4.17.13",
        "pace": "github:HubSpot/pace#v1.0.2",
        "perfect-scrollbar": "^1.4.0",
        "popper.js": "^1.15.0",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.1",
        "postcss-loader": "^6.1.1",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.21.0",
        "sass-loader": "^8.0.0",
        "sweetalert2": "^9.3.5",
        "tailwindcss": "^2.2.2",
        "vue": "^2.6.10",
        "vue-jest": "^3.0.4",
        "vue-template-compiler": "^2.6.10"
    },
    "browserslist": {
        "development": "last 1 version",
        "production": ">2%, Firefox ESR"
    },
    "jest": {
        "testURL": "http://localhost",
        "roots": [
            "<rootDir>/tests/Javascript/"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/resources/js/$1"
        },
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".*\\.(vue)$": "vue-jest"
        }
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.7.0",
        "add-event-to-calendar": "^1.0.7",
        "apexcharts": "^3.32.0",
        "classnames": "^2.2.6",
        "cytoscape": "^3.12.1",
        "datatables.net": "^1.10.21",
        "datatables.net-bs4": "^1.10.21",
        "echarts": "^5.2.2",
        "eonasdan-bootstrap-datetimepicker-bootstrap4beta": "^4.17.47-a",
        "eonasdan-custom-bootstrap-datetimepicker-with-bootstrap-4": "^1.0.0",
        "eventemitter3": "^4.0.0",
        "jstree": "^3.3.10",
        "lodash.debounce": "^4.0.8",
        "lodash.memoize": "^4.1.2",
